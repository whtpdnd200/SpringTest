<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.devwork.springtest.database.repository.StoreRepository">
    <select id="getStoreList"
    resultType="com.devwork.springtest.database.domain.Store">
        SELECT
            `id`
            , `name`
            , `phone_number`
            , `address`
            , `business_number`
            , `introduce`
            , `created_at`
            , `updated_at`
        FROM
            `store`
    </select>

</mapper>