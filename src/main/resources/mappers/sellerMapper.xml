<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.devwork.springtest.mvc.repository.SellerRepository">
    <insert id="insertSeller" parameterType="map">
        INSERT INTO
            `seller`
        (
            `nickname`
            , `profile_image`
            , `temperature`
        )
        VALUE
        (
            #{nickname}
            , #{image}
            , #{temperature}
        )
    </insert>
    <select id="selectLastSeller"
    resultType="com.devwork.springtest.mvc.domain.Seller">
        SELECT
            `id`
            , `nickname`
            , `temperature`
            , `profile_image`
            , `created_at`
            , `updated_at`
        FROM
            `seller`
        ORDER BY
            `id` DESC
            LIMIT 1
    </select>
</mapper>